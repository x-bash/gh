# shellcheck shell=sh disable=SC2039,SC2142,SC3043
xrc gh
x jq --help 1>/dev/null 2>&1
gh token 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
test_repo_main(){
    local repoName
    repoName="$(date +%s)"
    @assert false "(gh repo create)"
    printf "%s"  "x-cmd-test-$repoName" | @assert stdout gh repo create x-cmd-test-"$repoName" | x jq '.name'
    printf "%s" "x-cmd-test-$repoName" | @assert stdout gh user repo ls --sort created | x jq '.[2].name'
}

@assert:file:func